<!--pages/list/map/map/map.wxml-->
<map id="map" scale="14" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange" show-location style="width: 100%; height: 300px;"></map>
<button bindtap="getLocation" type="primary">点击获取当前地址位置</button>
